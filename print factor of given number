#include <stdio.h>
#include<conio.h>
int main()
{
        int n,i;
        printf("enter a positive integer");
        scanf("%d",&n);
        
        printf("factors of %d is",n);
        for(i=1;i<=n;++i)
        {
          if(n%i==0)
          {
              printf("%d\t",i);
          }
        }
return 0;
}
