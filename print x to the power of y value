#include <stdio.h>
#include<conio.h>
int main() 
{
    int x, y;
    long result = 1;

    printf("Enter the value of x: ");
    scanf("%d", &x);

    printf("Enter the value of y: ");
    scanf("%d", &y);

    for (int i = 0; i < y; i++) {
        result *= x;
    }

    printf("%d to the power of %d is %ld\n", x, y, result);

    return 0;
}


